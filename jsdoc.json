{
  "source": {
    "include": ["public"],
    "exclude": ["public/bebras-modules"]
  },
  "plugins": ["plugins/markdown"],

  "opts": {
    "destination": "../documentation_unixfilters/docs/",
    "recurse": true,
    "sort": false,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "fallback-light"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "theme_opts": {
    "footer": "This is footer"
  }
}
